{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "commonjs",
    "lib": ["DOM"],
    "moduleResolution": "Node",
    "rootDir": ".",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "alwaysStrict": true,
    "composite": true
  }
}
